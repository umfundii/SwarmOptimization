#  1000 iterations
echo "20 particles, 1000 iterations"
echo "RO:"
python3 knapsack.py examples/low-dimensional/f2_l-d_kp_20_878 20 1000 RO RI
echo "PSO:"
python3 knapsack.py examples/low-dimensional/f2_l-d_kp_20_878 20 1000 PSO RI
echo "DE:"
python3 knapsack.py examples/low-dimensional/f2_l-d_kp_20_878 20 1000 DE RI
echo "GWO:"
python3 knapsack.py examples/low-dimensional/f2_l-d_kp_20_878 20 1000 GWO RI
echo "Jaya:"
python3 knapsack.py examples/low-dimensional/f2_l-d_kp_20_878 20 1000 JAYA RI
echo "GA:"
python3 knapsack.py examples/low-dimensional/f2_l-d_kp_20_878 20 1000 GA RI
echo
echo

